	<queryString>
		<![CDATA[select * from view_pessoas_por_datas
WHERE
(isnull(sexo,'') = CASE WHEN $P{sexo} <> '' THEN $P{sexo} ELSE  isnull(sexo,'') END)
and
(isnull(ativo,'') = CASE WHEN $P{status} <> '' THEN $P{status} ELSE  isnull(ativo,'') END)
and
(isnull(mes,'') =  CASE WHEN $P{mes} <> '' THEN $P{mes} ELSE isnull(mes,'') END)
and
(empresas_id = CASE WHEN $P{empresas_id} <> 0 THEN $P{empresas_id} ELSE  empresas_id END)
and
(empresas_clientes_cloud_id = CASE WHEN $P{empresas_clientes_cloud_id} <> 0 THEN $P{empresas_clientes_cloud_id} ELSE  empresas_clientes_cloud_id END)
and
(isnull_int(estadoscivis_id,0) = CASE WHEN $P{estadoscivis} <> 0 THEN $P{estadoscivis} ELSE isnull_int(estadoscivis_id,0) END)
and
(isnull_int(tipos_pessoas_id,0) = CASE WHEN $P{tipos} <> 0 THEN $P{tipos} ELSE  isnull_int(tipos_pessoas_id,0) END)
and
(isnull_int(motivos_entrada_id,0) = CASE WHEN $P{motivo_entrada} <> 0 THEN $P{motivo_entrada} ELSE  isnull_int(motivos_entrada_id,0) END)
and
(isnull_int(motivos_saida_id,0) = CASE WHEN $P{motivo_saida} <> 0 THEN $P{motivo_saida} ELSE  isnull_int(motivos_saida_id,0) END)
and
(isnull_int(status_id,0) = CASE WHEN $P{status_id} <> 0 THEN $P{status_id} ELSE isnull_int(status_id,0) END)
and
(isnull_date(data_entrada,'1900/01/01') >=  CASE WHEN $P{data_entrada_inicial} <> '1900/01/01' THEN $P{data_entrada_inicial} ELSE isnull_date(data_entrada,'1900/01/01') END)
and
(isnull_date(data_entrada,'1900/01/01') <=  CASE WHEN $P{data_entrada_final} <> '1900/01/01' THEN $P{data_entrada_final} ELSE isnull_date(data_entrada,'1900/01/01') END)
and
(isnull_date(data_saida,'1900/01/01') >=  CASE WHEN $P{data_saida_inicial} <> '1900/01/01' THEN $P{data_saida_inicial} ELSE isnull_date(data_saida,'1900/01/01') END)
and
(isnull_date(data_saida,'1900/01/01') <=  CASE WHEN $P{data_saida_final} <> '1900/01/01' THEN $P{data_saida_final} ELSE isnull_date(data_saida,'1900/01/01') END)
and
(isnull_date(data_batismo,'1900/01/01') >=  CASE WHEN $P{data_batismo_inicial} <> '1900/01/01' THEN $P{data_batismo_inicial} ELSE isnull_date(data_batismo,'1900/01/01') END)
and
(isnull_date(data_batismo,'1900/01/01') <=  CASE WHEN $P{data_batismo_final} <> '1900/01/01' THEN $P{data_batismo_final} ELSE isnull_date(data_batismo,'1900/01/01') END)
and
(isnull_int(grupos_pessoas_id,0) = CASE WHEN $P{grupo} <> 0 THEN $P{grupo} ELSE isnull_int(grupos_pessoas_id,0) END)]]>
